<h1>Listing configures</h1>

<table>
  <thead>
    <tr>
      <th>Task</th>
      <th>S1</th>
      <th>S2</th>
      <th>S3</th>
      <th>S4</th>
      <th>If1</th>
      <th>If2</th>
      <th>If3</th>
      <th>If4</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @configures.each do |configure| %>
      <tr>
        <td><%= configure.task_id %></td>
        <td><%= configure.s1 %></td>
        <td><%= configure.s2 %></td>
        <td><%= configure.s3 %></td>
        <td><%= configure.s4 %></td>
        <td><%= configure.if1 %></td>
        <td><%= configure.if2 %></td>
        <td><%= configure.if3 %></td>
        <td><%= configure.if4 %></td>
        <td><%= link_to 'Show', configure %></td>
        <td><%= link_to 'Edit', edit_configure_path(configure) %></td>
        <td><%= link_to 'Destroy', configure, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Configure', new_configure_path %>
